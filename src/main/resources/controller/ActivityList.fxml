      </Label>
      <TableView fx:id="tableview" layoutX="26.0" layoutY="177.0" prefHeight="351.0" prefWidth="961.0">
         <columns>
            <TableColumn fx:id="ActivityListActivityIDColumn" onEditCommit="#activityidEditCommit" onEditStart="#activityidEditStart" prefWidth="93.0" text="Activity ID " />
            <TableColumn fx:id="ActivityListActivityNameColumn" onEditCommit="#activitynameEditCommit" onEditStart="#activitynameEditStart" prefWidth="295.0" text="Activity Name" />
